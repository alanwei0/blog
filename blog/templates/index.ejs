<!DOCTYPE html>
<html lang="en">
  <% include('head', { title: blogName }) %> 
<body>
  <div class="title"><%= blogName %> </div>
  <div class="post-list">
    <%= postGroups.forEach(({ year, posts }) => { %>
      <div class="post-group">
        <h2 class="year"><%= year %></h2>
        <ol>
          <%= posts.forEach(post => { %>
            <li><time><%= post.createTime %></time><a href="<%= post.url %>"><%= post.title %></a></li>
          <% }) %>
        </ol>
      </div>
    <% }) %>
  </div>
  <% include('footer') %> 
</body>
</html>